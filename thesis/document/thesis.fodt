   <text:p text:style-name="P182"><text:tab/>\input{../tab.dcf77}</text:p>
   <text:p text:style-name="P180"><text:tab/>\caption{<text:span text:style-name="T78">Poszczególne bity sygnału DCF77</text:span>} </text:p>
   <text:p text:style-name="P180"><text:tab/>\label{tab:dcf77sig} </text:p>
   <text:p text:style-name="P180">\end{table}</text:p>
   <text:p text:style-name="P177">\begin{figure}</text:p>
   <text:p text:style-name="P177"><text:tab/>\centering</text:p>
   <text:p text:style-name="P177"><text:tab/>\include<text:span text:style-name="T68">graphics</text:span>[width=<text:span text:style-name="T68">0.8</text:span>\textwidth]{../<text:span text:style-name="T68">images/</text:span>dcf<text:span text:style-name="T78">CarrierEnvelope</text:span>}</text:p>
   <text:p text:style-name="P178"><text:tab/>\caption<text:span text:style-name="T78">[Obwiednia sygnału DCF77]</text:span>{<text:span text:style-name="T78">Obwiednia sygnału DCF77 (źródło: \cite[s. 13]{bauch09dcf})</text:span>}</text:p>
